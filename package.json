{
  "name": "react-fi",
  "version": "1.0.15",
  "description": "React library for Feather icons.",
  "license": "Apache-2.0",
  "author": "MonkeyScript",
  "keywords": [
    "feather-icons",
    "react",
    "feather",
    "icons",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkeyscript/react-fi.git"
  },
  "bugs": {
    "url": "https://github.com/monkeyscript/react-fi/issues"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src/lib/index.d.ts"
  ],
  "types": "./src/lib/index.d.ts",
  "main": "./dist/react-fi.cjs",
  "module": "./dist/react-fi.js",
  "exports": {
    ".": {
      "types": "./src/lib/index.d.ts",
      "import": "./dist/react-fi.js",
      "require": "./dist/react-fi.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.0.1",
    "vitest": "^0.34.6"
  }
}
